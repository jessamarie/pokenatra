<div class="containareon">
  <h2>Create Trainer</h2>
  <form id="create_trainer" action="/trainers" method="post">

    <div>
      <label>Name: </label>
      <input type="text" name="trainer[name]">
    </div>

    <div>
      <label>Level: </label>
      <input type="text" name="trainer[level]">
    </div>

    <div>
      <label>Team_ID: </label>
      <input type="text" name="trainer[team_id]">
    </div>

    <div>
      <label>Image URL: </label>
      <input type="text" name="trainer[img_url]">
    </div>

    <h2><a
        href="javascript:{}"
        onclick="document.getElementById('create_trainer').submit();"
        type="submit">(Create)
    </a></h2>
  </form>

  <a href="/trainers">(Go Back)</a>
</div>
